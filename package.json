{
  "devDependencies": {
    "@cypress/grep": "^4.0.0",
    "@shelex/cypress-allure-plugin": "^2.40.0",
    "@testing-library/cypress": "^9.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/yargs": "^17.0.24",
    "allure-commandline": "^2.21.0",
    "async": "^3.2.4",
    "cypress-real-events": "^1.10.1",
    "cypress-split": "^1.3.15",
    "fs": "^0.0.1-security",
    "liquid-json": "^0.3.1",
    "lodash": "^4.17.21",
    "mocha-allure-reporter": "^1.4.0",
    "newman": "^5.3.2",
    "path": "^0.12.7",
    "pdf-parse": "^1.1.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4",
    "yargs": "^17.7.2"
  },
  "scripts": {
    "as-a": "npx as-a --config as-a.ini spec npx cypress run",
    "mocha:full": "npm run mocha:report && npm run mocha:merge && npm run mocha:html && npm run mocha:open:html",
    "mocha:report": "npx cypress run --reporter mochawesome --reporter-options reportDir='cypress/results',overwrite=false,html=false,json=true",
    "mocha:merge": "npx mochawesome-merge 'cypress/results/*.json' -o cypress/results/mochawesome.json'",
    "mocha:html": "npx marge cypress/results/mochawesome.json",
    "mocha:open:html": "start cypress/results/mochawesome-report/mochawesome.html",
    "runOnChrome": "cypress run --browser chrome --env allure=true && npx allure generate allure-results -o allure-report",
    "start": "npx cypress open",
    "test": "npx as-a spec npx cypress open ",
    "cy:parallel": "cypress-parallel -d cypress/e2e -t 5 -s cy:run",
    "cy:run": "cypress run",
    "run": "npx cypress run",
    "record": "npx cypress run --record --key d43973ab-655d-4198-9f44-11126ff677d3",
    "cy:parallel-record": "cypress-parallel -d cypress/e2e -t 5 -s cy:run --record --key d43973ab-655d-4198-9f44-11126ff677d3",
    "cy:ci":"npx cypress run --env LOGIN=${{ secrets.PREDPROD_LOGIN }},PASSWORD=${{ secrets.PREDPROD_PASSWORD }},TOKEN=${{ secrets.PREDPROD_TOKEN }}"    
  },
  "dependencies": {
    "as-a": "^2.2.2",
    "cypress": "^13.3.0",
    "axios": "^1.4.0",
    "cypress-parallel": "^0.13.0",
    "cypress-plugin-api": "^2.11.0",
    "cypress-testing-library": "^4.0.0",
    "dayjs": "^1.11.7",
    "fs-extra": "^11.1.1",
    "jsqr": "^1.4.0",
    "minimatch": "^9.0.3",
    "node": "^20.5.1",
    "number-format.js": "^2.0.9"
  },
  "name": "cypress",
  "version": "1.0.0",
  "main": "cypress.config.js",
